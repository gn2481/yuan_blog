<h1>管理者登入</h1>
  <%= form_with model:@admin, url: sign_in_admins_path ,local: true do |f| %>
    <div class="fields">
      <%= f.label :account %>
      <%= f.text_field :account, placeholder:'請輸入帳號' %>
    </div>
    <div class="fields">
      <%= f.label :password, placeholder:'請輸入密碼' %>
      <%= f.password_field :password %>
    </div>
    <%= f.submit "登入"%>
  <% end %> 
